AAA,CAAA,6CAAA,EAAA;AACA,QAAA,SAAA,EAAA,KAAA,GADA;AAEA,QAAA,SAAA,EAAA,QAAA,CAAA,KAAA,KAAA;AAFA,OAAA,EAIA,IAJA,CAIA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA;;AACA,QAAA,MAAA,CAAA,GAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAVA,EAWA,KAXA,CAWA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAbA;AAcA;AA/BA,GAZA;AA6CA,EAAA,OA7CA,qBA6CA;AAAA;;AACA,IAAA,KAAA,CACA,GADA,CACA,yCADA,EAEA,IAFA,CAEA,UAAA,QAAA;AAAA,aAAA,MAAA,CAAA,SAAA,CAAA,QAAA,CAAA,IAAA,CAAA;AAAA,KAFA,EAGA,KAHA,CAGA,UAAA,KAAA;AAAA,aAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA;AAAA,KAHA;AAIA;AAlDA,CAAA","sourcesContent":["<template>\n  <div class=\"Jadwal\">\n    <NavbarGuru />\n    <v-container>\n      <v-card max-width=\"800\" :elevation=\"6\" class=\"mx-auto my-12\">\n        <v-col md=\"12\">\n          <v-card-title>\n            <!-- <v-icon>mdi-google-classroom</v-icon> -->\n            Materi\n            <v-spacer></v-spacer>\n            <v-row justify=\"center\">\n              <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\n                <template v-slot:activator=\"{ on, attrs }\">\n                  <v-btn color=\"green\" dark small v-bind=\"attrs\" v-on=\"on\">\n                    Tambah\n                  </v-btn>\n                </template>\n                <v-card>\n                  <v-card-title>\n                    <span class=\"headline\">Tambah Materi</span>\n                  </v-card-title>\n                  <v-card-text>\n                    <v-container>\n                      <v-row>\n                        <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                          <v-text-field\n                            label=\"Mata Pelajaran\"\n                            v-model=\"mapel\"\n                            required\n                          ></v-text-field>\n                        </v-col>\n                        <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                          <v-text-field\n                            label=\"Materi\" v-model=\"des\"\n                            required\n                          ></v-text-field>\n                        </v-col>\n                        <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                          <!-- <v-text-field\n                            label=\"jurusan\"\n                            required\n                          ></v-text-field> -->\n                        </v-col>\n                      </v-row>\n                    </v-container>\n                  </v-card-text>\n                  <v-card-actions>\n                    <v-spacer></v-spacer>\n                    <v-btn color=\"red\" text @click=\"dialog = false\">\n                      batal\n                    </v-btn>\n                    <v-btn color=\"green\" text @click=\"tambahMateri\">\n                      Simpan\n                    </v-btn>\n                  </v-card-actions>\n                </v-card>\n              </v-dialog>\n            </v-row>\n          </v-card-title>\n\n          <v-simple-table>\n            <template v-slot:default>\n              <thead>\n                <tr>\n                  <th class=\"text-left\">No</th>\n                  <th class=\"text-left\">Mata Pelajaran</th>\n                  <th class=\"text-left\">Materi</th>\n                  <th class=\"text-left\">Jurusan/Kelas</th>\n                  <!-- <th class=\"text-left\">Edit</th> -->\n                </tr>\n              </thead>\n              <tbody>\n                <tr v-for=\"(materi, index) in materis\" :key=\"materi.id\">\n                  <td>{{index + 1}}</td>\n                  <td>{{ materi.matpel.name }}</td>\n                  <td>{{ materi.deskripsi }}</td>\n                  <td>{{ materi.matpel.jurusan.name }}</td>\n                  <td>\n                    <!-- <v-btn small class=\"mx-1\" dark color=\"green\"> Edit </v-btn> -->\n                    <v-btn\n                      type=\"submit\"\n                      small\n                      color=\"red\"\n                      dark\n                      class=\"mx-1\"\n                      @click=\"hapusMateri(materi.id)\"\n                    >\n                      Hapus\n                    </v-btn>\n                  </td>\n                </tr>\n                <!-- <v-divider></v-divider> -->\n              </tbody>\n            </template>\n          </v-simple-table>\n\n          <v-card-actions> </v-card-actions>\n        </v-col>\n      </v-card>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport NavbarGuru from \"../../components/user/NavbarGuru.vue\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    NavbarGuru,\n  },\n  data() {\n    return {\n      dialog: false,\n      materis: '',\n      mapel:'',\n      des:'',\n    };\n  },\n  methods: {\n    setMateri(data) {\n      this.materis = data;\n    },\n    hapusMateri(id) {\n      axios.delete(\"http://192.168.1.33:8080/sekolah/materi/\" + id)\n      .then(() => {\n        this.materis.splice(id, 1);\n        console.log(this.materis);\n      //updet materi\n        axios\n          .get(\"http://192.168.1.33:8080/sekolah/materi\")\n          .then((response) => this.setMateri(response.data))\n          .catch((error) => console.log(error));\n      });\n    },\n    tambahMateri(){\n      axios.post(\"http://192.168.1.33:8080/sekolah/materi/add\",{\n        deskripsi: this.des,\n        id_matpel: parseInt(this.mapel)\n      })\n      .then(res=>{\n        this.setMateri();\n        this.des = '';\n        this.mapel = parseInt();\n        this.dialog = false;\n        console.log(res);\n      })\n      .catch(err=> {\n        console.log(err);\n      })\n    },\n  },\n  mounted() {\n    axios\n      .get(\"http://192.168.1.33:8080/sekolah/materi\")\n      .then((response) => this.setMateri(response.data))\n      .catch((error) => console.log(error));\n  },\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/views/guru"}]}